{
  "builds": [
    {
      "src": "Backend/NoteCanvas/NoteCanvas/wsgi.py",
      "use": "@natanffs/vercel-python-wsgi",
      "config": {
        "maxLambdaSize": "15mb",
        "runtime": "python3.9"
      }
    },
    {
      "src": "Backend/NoteCanvas/build_files.sh",
      "use": "@vercel/static-build",
      "config": {
          "distDir": "staticfiles_build"
       }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "Backend/NoteCanvas/NoteCanvas/wsgi.py"
    }
  ]
}
